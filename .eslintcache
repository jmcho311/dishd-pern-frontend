mport PostCard from '../components/PostCard';\nimport PostModel from '../models/post'\nimport UserModel from '../models/user'\n\n\nconst Profile = props => {\n  const [posts, setPosts] = useState([])\n  const [userId] = useState(props.currentUser)\n  const [user, setUser] = useState([])\n  const [wasDeleted, setWasDeleted] = useState(false)\n\n  useEffect(() => {\n    fetchPosts()\n    fetchUserInfo()\n  }, [])\n\n  useEffect(() => {\n    // console.log('ran this thing')\n    fetchPosts()\n  }, [wasDeleted])\n\n  const fetchPosts = () => {\n    PostModel.showPost(userId).then((data) => {\n      setPosts(data.posts)\n    })\n  }\n\n  const fetchUserInfo = () => {\n    UserModel.index().then((data) => {\n      setUser(data.user[0].name)\n    })\n  }\n\n  const deletedPost = (postId) => {\n    // console.log(postId)\n    PostModel.delete(postId).then((data) => {\n      setWasDeleted(!wasDeleted)\n    })\n  }\n\n  const generatePosts = () => {\n    return posts.map((post, index) => (\n      <div key={index}>\n        <PostCard { ...post } deletedPost={ deletedPost } fetchPosts={ fetchPosts }/>\n      </div>\n    ))\n  }\n  \n\n  return (\n    <div>\n      <h1>Profile Page for { user }</h1>\n      { posts.length ? generatePosts() : \"Loading...\" }\n    </div>\n  )\n}\n\nexport default Profile","/Users/jmcho311/Desktop/SEIR921/Unit4/final-project/dishd-pern-frontend/src/components/PostCard.js",[],"/Users/jmcho311/Desktop/SEIR921/Unit4/final-project/dishd-pern-frontend/src/models/post.js",[],"/Users/jmcho311/Desktop/SEIR921/Unit4/final-project/dishd-pern-frontend/src/pages/CreateNew.js",[],"/Users/jmcho311/Desktop/SEIR921/Unit4/final-project/dishd-pern-frontend/src/pages/CategoryShow.js",["72"],"/Users/jmcho311/Desktop/SEIR921/Unit4/final-project/dishd-pern-frontend/src/components/ResultsCard.js",[],"/Users/jmcho311/Desktop/SEIR921/Unit4/final-project/dishd-pern-frontend/src/pages/Search.js",[],{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":